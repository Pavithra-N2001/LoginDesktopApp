<Window x:Class="LoginDesktopApp.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:LoginDesktopApp"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Horizontal">
            <Button Click="Login_Click" x:Name="Login" Margin="20,0,20,20" VerticalAlignment="Top" Content="Login" Foreground="White" FontSize="15" Background="#545d6a" Height="28" Width="158" />
            <Button Click="reg_Click" x:Name="reg"  Margin="20,0,20,20" Content="Sign Up" VerticalAlignment="Top" Foreground="White" FontSize="15" Background="#545d6a" Height="28" Width="158" />
        </StackPanel>
        <Frame x:Name="Main" Margin="0,34,0,0"></Frame>

    </Grid>
</Window>
